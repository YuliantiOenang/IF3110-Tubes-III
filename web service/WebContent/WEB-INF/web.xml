<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>BarangController</servlet-name>
		<servlet-class>com.frexescwebservice.controller.BarangController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BarangUserController</servlet-name>
		<servlet-class>com.frexescwebservice.controller.BarangUserController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>KategoriController</servlet-name>
		<servlet-class>com.frexescwebservice.controller.KategoriController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserController</servlet-name>
		<servlet-class>com.frexescwebservice.controller.UserController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BarangController</servlet-name>
		<url-pattern>/barang</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BarangUserController</servlet-name>
		<url-pattern>/baranguser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>KategoriController</servlet-name>
		<url-pattern>/kategori</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserController</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index</welcome-file>
	</welcome-file-list>
</web-app>